<%include ../partials/header%>
<header class="header-base">
  <div class="container">
    <h1>Messages</h1>
    <h2>234 messages</h2>
  </div>
</header>
<main>
  <section class="section-base">
    <div class="container">
      <div class="row">
        <div class="col-lg-3">
          <div class="menu-inner menu-inner-vertical">
            <ul>
              <li>
                <a href="/dashboard">Dashboard</a>

              </li>
              <li>
                <a href="/dashboard/messages">Messages</a>

              </li>
              <li>
                <a href="/dashboard/subscribers">Subscribers</a>

              </li>
              <li class="active">
                <a href="/dashboard/register">Register</a>

              </li>

            </ul>
          </div>
        </div>
        <div class="col-lg-9">
          <h2>Register a new Admin<span class="dot">.</span></h2>
          <hr class="space-sm">
          <form id="registerationForm" class="form-box" method="POST">

            <div class="row">
              <div class="col-lg-12">
                <div id="reg_success" style="display: none;"></div>
              </div>
              <div class="col-lg-6">
                <label for="name">Name</label>
                <input id="name" name="name" placeholder="" type="text" class="input-text mb-0">
                <span class="invalid-feedback" id='name_error'></span>

              </div>
              <div class="col-lg-6">
                <label for="email">Email</label>
                <input id="email" name="email" placeholder="" type="email" class="input-text mb-0">
                <span class="invalid-feedback" id='email_error'></span>

              </div>
              <div class="col-lg-6 ">
                <hr class="space-xs" />
                <label for="email">Password</label>
                <input id="password" name="password" placeholder="" type="password" class="input-text mb-0">
                <span class="invalid-feedback" id='password_error'></span>

              </div>
            </div>
            <hr class="space-sm" />

            <button class="btn btn-sm btn-circle" type="submit">Register</button>

          </form>
        </div>
        <hr class="space-lg" />

      </div>
  </section>
  <script src="/../../themekit/scripts/progress.js"></script>
</main>

<%include ../partials/footer%>